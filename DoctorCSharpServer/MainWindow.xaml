<Window x:Class="DoctorClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodels="clr-namespace:DoctorClient.ViewModels"
        xmlns:views="clr-namespace:DoctorClient.Views"
        xmlns:local="clr-namespace:DoctorClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Resources>
        <DataTemplate x:Name="modifyViewTemplate" DataType="{x:Type viewmodels:ModifyModel}">
            <views:ModifyView DataContext="{Binding}"/>
        </DataTemplate>
        <DataTemplate x:Name="homeViewTemplate" DataType="{x:Type viewmodels:HomeModel}">
            <views:HomeView DataContext="{Binding}"/>
        </DataTemplate>
    </Window.Resources>
    <Grid>
        <ContentControl Content="{Binding}" HorizontalAlignment="Center" VerticalAlignment="Center" Height="434" Width="800"/>



    </Grid>
</Window>
